public class Exer7 {

    public static void main(String[] args){

        //Write a code in which you display all integers from -100 to 100,
        // except those divisible by 2 or 3.

        int number = -100;
        while (number < 100) {
            if (number % 2==0 || number % 3 == 0){
                number++;
                continue;
            }
            System.out.println(number);
            number++;
        }

    }
}
